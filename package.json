{
  "name": "bookshelf-next-frontend",
  "private": "true",
  "scripts": {
    "dev": "run-p -n dev:*",
    "dev:codegen": "yarn build:codegen -ws",
    "dev:next": "next dev -p 3000",
    "build": "run-s build:gqlgen build:next",
    "build:codegen": "gql-gen",
    "build:next": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "run-s -cn lint:*",
    "lint:eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:stylelint": "stylelint src/**/*.{css,scss,sass,jsx,tsx}",
    "lint:prettier": "prettier --check *.{json,yml,yaml,md,gql,graphql}",
    "fmt": "run-s -cn fmt:*",
    "fmt:eslint": "yarn lint:eslint --fix",
    "fmt:stylelint": "yarn lint:stylelint --fix",
    "fmt:prettier": "yarn lint:prettier --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "engines": {
    "node": "12.x",
    "yarn": "1.x"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "apollo-boost": "^0.4.9",
    "clsx": "^1.1.0",
    "dayjs": "^1.8.35",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.2.6",
    "graphql-tag": "^2.11.0",
    "i18next": "^19.6.3",
    "i18next-browser-languagedetector": "^6.0.1",
    "isbn3": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "next": "^9.5.2",
    "react": "^16.13.1",
    "react-dom": "16.13.1",
    "react-i18next": "^11.7.0",
    "styled-components": "^5.1.1",
    "tailwindcss": "^1.7.1",
    "type-fest": "^0.16.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.2",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/introspection": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-document-nodes": "^1.17.8",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@shopify/eslint-plugin": "^37.0.1",
    "@shopify/prettier-config": "^1.1.0",
    "@shopify/stylelint-plugin": "^9.0.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "autoprefixer": "^9.8.6",
    "browserslist": "^4.14.0",
    "eslint": "7.7.0",
    "jest": "^26.4.0",
    "npm-run-all": "4.1.5",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-import": "^12.0.1",
    "postcss-will-change": "3.0.0",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.9.7"
  }
}
